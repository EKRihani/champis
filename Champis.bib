	file = {Texte intégral:/home/ekr/Zotero/storage/L8QXQHCP/Viktorova et al. - 2016 - Native Phytoremediation Potential of Urtica dioica.pdf:application/pdf},
}

@article{khalili_antihypoxic_2015,
	title = {Antihypoxic activities of {Eryngium} caucasicum and {Urtica} dioica},
	volume = {19},
	issn = {2284-0729},
	abstract = {OBJECTIVE: Urtica dioica and Eryngium spp. have been used in traditional medicine for many years. In spite of many works, nothing is known about their protective effect against hypoxia-induced lethality.
MATERIALS AND METHODS: Protective effects of U. dioica (UD) aerial parts and E. caucasicum (EC) inflorescence against hypoxia-induced lethality in mice were evaluated by three experimental models of hypoxia, asphyctic, haemic and circulatory.
RESULTS: Statistically significant protective activities were established in some doses of extracts in three models. Antihypoxic activity was especially pronounced in polyphenol fractions in asphyctic model. EC polyphenol fraction at 400 mg/kg prolonged survival time (48.80 ± 4.86, p {\textless} 0.001) which was comparable with that of phenytoin (p {\textgreater} 0.05). It was the most effective extract in circulatory model, too. It prolonged survival time significantly respect to control group (p {\textless} 0.001). UD extracts protected the mice but the response was not dose-dependent. In haemic model, extracts of EP signiﬁcantly and dose dependently prolonged survival time as compared to control group (p {\textless} 0.001). At 600 mg/kg, EP was the most effective one, being capable of keeping the mice alive for 12.71 ± 0.75 min. Only the concentration of 300 mg/kg of UD was effective (p {\textless} 0.001).
CONCLUSIONS: Extracts showed remarkable antihypoxic effects. Pharmacological effects may be attributed to the presence of polyphenols in the extracts.},
	language = {eng},
	number = {17},
	journal = {European Review for Medical and Pharmacological Sciences},
	author = {Khalili, M. and Dehdar, T. and Hamedi, F. and Ebrahimzadeh, M. A. and Karami, M.},
	month = sep,
	year = {2015},
	pmid = {26400535},
	keywords = {Animals, Eryngium, Hypoxia, Male, Mice, Plant Extracts, Polyphenols, Urtica dioica},
	pages = {3282--3285},
	file = {Khalili et al. - 2015 - Antihypoxic activities of Eryngium caucasicum and .pdf:/home/ekr/Zotero/storage/H5U258ZF/Khalili et al. - 2015 - Antihypoxic activities of Eryngium caucasicum and .pdf:application/pdf},
