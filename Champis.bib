	journal = {Iranian Journal of Pharmaceutical Research : IJPR},
	author = {Bakhshaee, Mehdi and Mohammad pour, Amir Hooshang and Esmaeili, Majid and Jabbari Azad, Farahzad and Alipour Talesh, Ghazal and Salehi, Maryam and Noorollahian Mohajer, Morteza},
