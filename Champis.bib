@article{bakhshaee_efficacy_2017,
	title = {Efficacy of {Supportive} {Therapy} of {Allergic} {Rhinitis} by {Stinging} {Nettle} ({Urtica} dioica) root extract: a {Randomized}, {Double}-{Blind}, {Placebo}- {Controlled}, {Clinical} {Trial}},
