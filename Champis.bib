}

@article{testai_cardiovascular_2002,
	title = {Cardiovascular effects of {Urtica} dioica {L}. ({Urticaceae}) roots extracts: in vitro and in vivo pharmacological studies},
	volume = {81},
	issn = {03788741},
	shorttitle = {Cardiovascular effects of {Urtica} dioica {L}. ({Urticaceae}) roots extracts},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378874102000557},
	doi = {10.1016/S0378-8741(02)00055-7},
	abstract = {Urtica dioica (Urticaceae) is a plant principally used in the traditional medicine of oriental Marocco as antihypertensive remedy (J. Ethnopharmacol., 58 (1997), 45). The aim of this work was to evaluate a possible direct cardiovascular action of the plant and to investigate its mechanism of action. In aortic preparations with intact and functional endothelial layer, pre-contracted with KCl 20 mM or norepinephrine 3 mM, the crude aqueous and methanolic extracts of the plant roots, as well as purified fractions elicited a vasodilator action. Nevertheless, the vasodilator activity was not present in aortic rings without endothelial layer. In aortic rings with intact endothelial layer, the vasorelaxing effect was abolished by L-NAME, a NO-biosynthesis inhibitor, and ODQ, a guanylate cyclase inhibitor. Furthermore, potassium channel blockers (TEA, 4-aminopyridine, quinine, but not glybenclamide) antagonized the vasodilator action of the purified fraction F1W of U. dioica . The same fraction produced a marked decrease of inotropic activity, in spontanoeusly beating atria of guinea-pig, and a marked, but transient, hypotensive activity on the blood pressure of anaesthetized rats. It is concluded that U. dioica can produce hypotensive responses, through a vasorelaxing effect mediated by the release of endothelial nitric oxide and the opening of potassium channels, and through a negative inotropic action. \# 2002 Elsevier Science Ireland Ltd. All rights reserved.},
	language = {en},
	number = {1},
	urldate = {2020-08-05},
	journal = {Journal of Ethnopharmacology},
	author = {Testai, Lara and Chericoni, Silvio and Calderone, Vincenzo and Nencioni, Giulia and Nieri, Paola and Morelli, Ivano and Martinotti, Enrica},
	month = jun,
	year = {2002},
	pages = {105--109},
	file = {Testai et al. - 2002 - Cardiovascular effects of Urtica dioica L. (Urtica.pdf:/home/ekr/Zotero/storage/P7E2EJ8X/Testai et al. - 2002 - Cardiovascular effects of Urtica dioica L. (Urtica.pdf:application/pdf},
}

