	journal = {Biomedicine \& Pharmacotherapy},
	author = {Mzid, Massara and Ghlissi, Zohra and Salem, Maryem Ben and Khedir, Sameh Ben and Chaabouni, Khansa and Ayedi, Fatma and Sahnoun, Zouheir and Hakim, Ahmed and Rebai, Tarek},
