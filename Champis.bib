@article{tekin_examination_2020,
	title = {Examination of the {Association} {Between} 3,4-{Divanillyltetrahydrofuran} {Lignan} ({Urtica} dioica {Origin}) and {Prostate} {Cancer} {Cells} by {131I} {Radiolabeling}},
