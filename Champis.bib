	journal = {Pharmaceutical Biology},
	author = {Zemmouri, Hanene and Sekiou, Omar and Ammar, Sonda and El Feki, Abdelfattah and Bouaziz, Mohamed and Messarah, Mahfoud and Boumendjel, Amel},
