	pages = {533--541},
	file = {Ngugi et al. - 2015 - Effects of dietary administration of stinging nett.pdf:/home/ekr/Zotero/storage/6648XYUH/Ngugi et al. - 2015 - Effects of dietary administration of stinging nett.pdf:application/pdf},
