	abstract = {The present study was aimed to evaluate the effect of Urtica dioica (UD) extract against chronic unpredictable stress (CUS)-induced associative memory dysfunction and attempted to explore the possible mechanism. Male Swiss albino mice (25–30 g) were divided into six groups, viz. group-I received 0.3\% carboxymethyl cellulose and served as control (CTRL), group II was exposed to CUS (21 days) and received vehicle (CUS), group III was subjected to CUS and received Hypericum perforatum extract (350 mg/kg, p.o.) (CUS + HYP), group IV received Hypericum perforatum extract (350 mg/kg, p.o.) (CTRL + HYP); group V was subjected to CUS and received UD extract (50 mg/kg, p.o.) (CUS + UD), group VI received UD extract (50 mg/kg, p.o.) (CTRL + UD). CUS signiﬁcantly induced body weight loss (p {\textless} 0.05) and associative memory impairment in step down task (p {\textless} 0.05) as compared to control mice. CUS signiﬁcantly downregulated Smo (p {\textless} 0.05), Gli1 (p {\textless} 0.01), cyclin D1 (p {\textless} 0.05), BDNF (p {\textless} 0.01), TrKB (p {\textless} 0.01) and MAPK1 (p {\textless} 0.01) mRNA expression in hippocampus as compared to control mice. CUS signiﬁcantly increased the levels of TBARS (p {\textless} 0.01) and nitric oxide (p {\textless} 0.001), and decreased catalase (p {\textless} 0.001) and total thiol (p {\textless} 0.01) in plasma resulting in oxidative stress and inﬂammation. Chronic UD administration signiﬁcantly reverted CUS mediated body weight loss (p {\textless} 0.05) and cognitive impairment (p {\textless} 0.05). UD administration signiﬁcantly decreased the levels of TBARS (p {\textless} 0.01) and nitric oxide (p {\textless} 0.05), and increased the levels of catalase (p {\textless} 0.01) and total thiol (p {\textless} 0.05) in plasma. Chronic UD administration signiﬁcantly upregulated hippocampal Smo (p {\textless} 0.05), Gli1 (p {\textless} 0.001), cyclin D1 (p {\textless} 0.05), BDNF (p {\textless} 0.05), TrKB (p {\textless} 0.05) and MAPK1 (p {\textless} 0.05) in stressed mice. Further, UD extract did not reverse cyclopamine induced downregulation of Gli1 and Ptch1 mRNA in hippocampal slices. UD modulated Smo-Gli1 pathway in the hippocampus as well as exerted anti-inﬂammatory and antioxidant effects. UD extract might prove to be effective for stress mediated neurological disorders.},
	language = {en},
	urldate = {2020-07-05},
	journal = {Biomedicine \& Pharmacotherapy},
	author = {Patel, Sita Sharan and Mahindroo, Neeraj and Udayabanu, Malairaman},
	month = oct,
	year = {2016},
	pages = {676--686},
	file = {Patel et al. - 2016 - Urtica dioica leaves modulates hippocampal smoothe.pdf:/home/ekr/Zotero/storage/SVDN5R9Y/Patel et al. - 2016 - Urtica dioica leaves modulates hippocampal smoothe.pdf:application/pdf},
}

@article{irgin_does_2016,
	title = {Does stinging nettle ({Urtica} dioica) have an effect on bone formation in the expanded inter-premaxillary suture?},
