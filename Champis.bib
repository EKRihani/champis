	keywords = {Animals, Plant Extracts, Urtica dioica, Antineoplastic Agents, Phytogenic, breast cancer, cancer therapy, Drug Evaluation, Preclinical, food bioactives, Humans, natural products bioactivity, nutraceuticals, Phytochemicals, Structure-Activity Relationship},
