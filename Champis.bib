	pmid = {32523882},
	pmcid = {PMC7256274},
	pages = {273--286},
	file = {Hajihashemi et al. - 2020 - Ameliorative effect of cotreatment with the methan.pdf:/home/ekr/Zotero/storage/5NJG9W6X/Hajihashemi et al. - 2020 - Ameliorative effect of cotreatment with the methan.pdf:application/pdf},
}

