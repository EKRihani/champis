	pages = {95},
	file = {Flores-Ocelotl et al. - 2018 - Taraxacum officinale and Urtica dioica extracts in.pdf:/home/ekr/Zotero/storage/X2R2ELTF/Flores-Ocelotl et al. - 2018 - Taraxacum officinale and Urtica dioica extracts in.pdf:application/pdf},
}

